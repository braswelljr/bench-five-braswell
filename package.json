{
  "name": "bench-five-braswell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx",
    "preview": "vite preview",
    "lint:fix": "npx eslint ./**/*.{js,ts,jsx,tsx} --quiet --fix",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,tsx,jsx}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write ./**/*.{js,ts,jsx,tsx,css}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-toast": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "firebase": "^9.21.0",
    "framer-motion": "^10.12.4",
    "match-sorter": "^6.3.1",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-hooks": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --write --list-different"
  }
}
